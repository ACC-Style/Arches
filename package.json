		"color_codes": "kss --config kss-config_color_codes.json",
		"_all": "kss --config kss-config.json && kss --config kss-config_uc_only.json && kss --config kss-config_zurb_acc.json  && kss --config kss-config_boot_cvquality.json && kss --config kss-config_layout_demo.json && kss --config kss-config_color_codes.json"
	},
	"repository": {
